{
    "name": "alamindev/logviewer",
    "description": "Log Viewer is a package for Laravel allowing you to easily read and manage your Laravel logs",
    "type": "library",
    "authors": [
        {
            "name": "alamin",
            "email": "alamincsetpi@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Alamin\\Logviewer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alamin\\Logviewer\\LogViewerServiceProvider"
            ]
        }
    },
    "require": {
        "alamindev/logviewer": "dev-master"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
